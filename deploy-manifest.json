{
  "version": 1,
  "framework": { "name": "react", "version": "18.2.0" },
  "imageSettings": {
    "sizes": [
      100,
      200,
      1920
    ],
    "domains": ["images.unsplash.com", "image.pbs.org", "image.pbskids.org"],
    "remotePatterns": [
      {
        "protocol": "https",
        "hostname": "image.pbs.org",
        "port": ""
      },
      {
        "protocol": "https",
        "hostname": "image.pbskids.org",
        "port": ""
      }
    ],
    "formats": ["image/jpeg", "image/png", "image/webp", "image/avif"],
    "minimumCacheTTL": 60,
    "dangerouslyAllowSVG": false
  },
  "routes": [
    {
      "path": "/assets/index-4c4b02f8.css",
      "target": {
        "kind": "Compute",
        "src": "default"
      },
      "fallback": {
        "kind": "Static",
        "cacheControl": "public, max-age=28"
      }
    },
    {
      "path": "/assets/*",
      "target": {
        "kind": "Static",
        "cacheControl": "public, max-age=31536000, immutable"
      }
    },
    {
      "path": "/screenshots/ss1.png",
      "target": {
        "kind": "Static",
        "cacheControl": "public, max-age=9"
      }
    },
    {
      "path": "/screenshots/sspc1.png",
      "target": {
        "kind": "Static",
        "cacheControl": "public, max-age=422"
      }
    },
    {
      "path": "/screenshots/performance.png",
      "target": {
        "kind": "ImageOptimization"
      },
      "fallback": {
        "kind": "Static",
        "cacheControl": "public, max-age=89"
      }
    },
    {
      "path": "/screenshots/*",
      "target": {
        "kind": "Static",
        "cacheControl": "public, max-age=0"
      }
    },
    {
      "path": "/todo*",
      "target": {
        "kind": "Static",
        "cacheControl": "public, max-age=0"
      }
    },
    {
      "path": "/ahio",
      "target": {
        "kind": "ImageOptimization"
      }
    },
    {
      "path": "/ahioCacheControl",
      "target": {
        "kind": "ImageOptimization",
        "cacheControl": "max-age=35"
      }
    },
    {
      "path": "/staticToAhio",
      "target": {
        "kind": "Static"
      },
      "fallback": {
        "kind": "ImageOptimization"
      }
    },
    {
      "path": "/computeToAhio",
      "target": {
        "kind": "Compute",
        "src": "default"
      },
      "fallback": {
        "kind": "ImageOptimization"
      }
    },
    {
      "path": "/*",
      "target": {
        "kind": "Compute",
        "src": "default"
      },
      "fallback": {
        "kind": "Static",
        "cacheControl": "public, max-age=0"
      }
    }
  ],
  "computeResources": [
    {
      "name": "default",
      "runtime": "nodejs18.x",
      "entrypoint": "index.cjs"
    }
  ]
}
